####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	test -d bin/ || mkdir -p bin/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: linux.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile linux.pro
qmake: 
	@$(QMAKE) -o Makefile linux.pro

dist: 
	@mkdir -p tmp/obj/cutter && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) tmp/obj/cutter/ && $(COPY_FILE) --parents src/qtGUI/optionswindow.ui.h tmp/obj/cutter/ && ( cd `dirname tmp/obj/cutter` && $(TAR) cutter.tar cutter && $(GZIP) cutter.tar ) && $(MOVE) `dirname tmp/obj/cutter`/cutter.tar.gz . && $(DEL_FILE) -r tmp/obj/cutter

